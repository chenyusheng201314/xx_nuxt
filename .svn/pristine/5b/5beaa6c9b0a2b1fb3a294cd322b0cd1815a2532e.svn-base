        company:'',
      }
    },
    async asyncData({store,params}) {
      let id = Number(params.id)
      let lessons = []
      // console.log('在线微课',id)
      if (id===10) {
        // 企业内训中的 课程
        let res = await store.dispatch('httpGet',{
          url: '/home/course/course_list',
          method:'get',
          params:{
            page:1,
            pasize:100,
            sort_id:''
          }
        })

        let dd = res.data.data
        // console.log('在线微课0',Math.ceil(dd.length/8))
        let len = Math.ceil(dd.length/8)
        if(len) {
          for(let i=0;i< len;i++){
            let d = JSON.parse(JSON.stringify(dd))
            let a = []
            a = d.splice(i*8,8)
            lessons.push(a)
          }
        }
      }
      // console.log('在线微课11',lessons)
      return {
        id,
        lessons
      }
    },
