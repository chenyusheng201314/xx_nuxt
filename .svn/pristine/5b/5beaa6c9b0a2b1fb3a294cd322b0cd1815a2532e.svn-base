      show_modal () {
        this.modal = true
        document.getElementsByTagName("body")[0].className = 'modal-open'
      },
      // 在线沟通 窗口
      open_user () {
        window.open('http://p.qiao.baidu.com/cps/chat?siteId=12667303&userId=26578884','_blank',
          "height=600, width=900, top=200, left=600, scrollbars=no,resizable=0.2,modal=false,alwaysRaised=yes");
      },
      close_modal() {
        this.modal = false
        document.getElementsByTagName("body")[0].className = ''
      },
      // 报名
      async sign() {
        // alert('提交')
        if (!this.sign_title) {
          // alert('请填写课程名')
          this.$message({
            type:'warning',
            message:'请填写课程名~'
          })

        }else if (!this.customer_name){
          this.$message({
            type:'warning',
            message:'请填写姓名~'
          })
        }else if (!this.iphone(this.mobile)){
          this.$message({
            type:'warning',
            message:'请填写合法的手机号~'
          })
        }else {
          let res = await this.$store.dispatch('httpPost', {
            url:'/home/sign_up_msg/msg_add',
            data:{
              sign_title:this.sign_title,
              customer_name:this.customer_name,
              mobile:this.mobile,
              company:this.company
            }
          });
          if(res.code===0){
            this.$message({
              type:'success',
              message:'报名成功~'
            })
            setTimeout(()=>{
              this.close_modal()
            },1500)
            this.customer_name=''
            this.mobile=''
            this.company=''
          }else {
            this.$message({
              type:'error',
              message:res.message
            })
          }

        }
      }
    },
    beforeMount () {
      // <!--id = 1 基于岗位序列的课程体系设计 2绩效改进技术  3教学案例设计与开发-->
      // <!--4柯氏四级培训效果评估 5完美授课技巧 6完美咨询技巧 7在岗带教-->
      // 有报名信息的时候 要填写sign_title
      switch (this.id) {
        case 1:
          this.sign_title='基于岗位序列的课程体系设计'
          break;
        case 2:
          this.sign_title='绩效改进技术'
          break;
        case 3:
          this.sign_title='教学案例设计与开发'
          break;
        case 4:
          this.sign_title='柯氏四级培训效果评估'
          break;
        case 5:
          this.sign_title='完美授课技巧'
          break;
        case 6:
          this.sign_title='完美咨询技巧'
          break;
        case 7:
          this.sign_title='在岗带教'
          break;

        case 13:
          this.sign_title='TDF'
          break;



        case 21:
          this.sign_title='房产行业'
          break;
        case 22:
          this.sign_title='服装行业'
          break;
        case 23:
          this.sign_title='金融行业'
          break;
        case 24:
          this.sign_title='汽车行业'
          break;
        case 25:
          this.sign_title='物流行业'
          break;
        case 26:
          this.sign_title='医药行业'
          break;
        case 27:
          this.sign_title='IT行业'
          break;
      }
    },
    mounted () {
      // 企业内训 轮播图
      if (this.id===10) {
        this.init()
      }
    }
  }
</script>
