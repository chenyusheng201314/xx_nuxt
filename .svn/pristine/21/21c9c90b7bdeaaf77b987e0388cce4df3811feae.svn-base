p {
  margin: 0;
}

.case-detail-con {
  width: 1200px;
  margin: 0 auto;
}

.case-detail-tit {
  width: 1200px;
  height: 45px;
  background: #eee;
  padding: 0 45px;
  display: flex;
  flex-direction: row;
  justify-content: space-around;
}

.case-detail-tit .case-tit-con {
  float: left;
  padding: 0 10px;
  height: 45px;
  line-height: 45px;
  font-size: 18px;
  color: #333;
  /*margin-right: 55px;*/
  text-decoration: none;
}

.case-detail-tit .case-tit-con:last-child {
  margin-right: 0;
}
.ql-editor {
  color: #666666;
  padding: 12px 0 0;
}
.case-detail-tit .case-tit-con.active {
  height: 50px;
  background: #fff;
  border: 1px solid #dcdcdc;
  color: #ff6900;
  border-bottom: 2px solid #ff6900;
  margin-top: -5px;
  line-height: 50px;
}
.tit-introduce {
  color: #666666;
  background-color: #eee;
  font-size: 14px;
  text-indent: 2em;
  padding: 28px;
}
.case-detail-article {
  padding: 0 36px 36px;
  border: 1px solid #dcdcdc;
  /*border-bottom: none;*/
  margin-top: 30px;
}

.case-detail-article>li {
  padding: 45px 0;
  border-bottom: 1px solid #dcdcdc;
}
.case-detail-article>li:last-of-type {
  border-bottom: 0 none;
}

.case-detail-article li h3 {
  width: 100%;
  height: auto;
  text-align: center;
  color: #ff6900;
  font-size: 20px;
  font-weight: bold;
  margin-bottom: 30px;
}

.case-detail-article li p.name,
.other-case p.name {
  font-size: 18px;
  line-height: 18px;
  color: #ff6900;
  /*margin-top: 20px;*/
  /*margin-bottom: 10px;*/
}

.case-detail-article li p.con {
  font-size: 15px;
  color: #666;
}

.other-case {
  margin-top: 20px;
  border: 1px solid #dcdcdc;
  padding: 45px;
  margin-bottom: 50px;
}

.other-case ul {
  width: 100%;
  height: auto;
  overflow: hidden;
  margin-top: 15px;
  margin-bottom: 0;
  padding: 0;
}

.other-case ul li {
  display: inline-block;
  width: 208px;
  margin-right: 13px;
  margin-top: 15px;
  cursor: pointer;
  vertical-align: top;
}
.other-case ul li p{
  text-align: center;
  font-size: 14px;
  color: #333;
}
.case {
  width: 1200px;
  margin: 0 auto;
}

.case-list {
  width: 1200px;
  height: auto;
  padding: 25px 45px 45px;
  border: 1px solid #dcdcdc;
  box-sizing: border-box;
  -webkit-box-sizing: border-box;
  -moz-box-sizing: border-box;
  -ms-box-sizing: border-box;
  -o-box-sizing: border-box;
  margin-bottom: 50px;
}

.case-list li {
  margin-top: 45px;

}

.case-list li:first-child {
  margin-top: 0;
}

.case-list li h2 {
  font-size: 20px;
  color: #ff6900;
}

.case-list li .img-all {
  margin-top: 22px;
  overflow: hidden;
}

.case-list li .img-all li {
  width: 265px;
  margin-right: 5px;
  margin-left: 5px;
  margin-top: 20px;
  cursor: pointer;
  display: inline-block;
  vertical-align: top;
}

.case-list li .img-all img {
  width: 265px;
  height: 150px;
}
.case-li-img {
  width: 265px;
  height: 150px;
  background:  no-repeat center;
  background-size: cover;
}
.case-list li .img-all p {
  margin-top: 15px;
  margin-bottom: 0;
  font-size: 16px;
  text-align: center;
}

.go {
  top: 50%;
  font-size: 24px;
  color: #999;
  cursor: pointer;
}
.pre{
  left: 0;
}
.next {
  right: 0;
}
.gray {
  color: #eee;
}
