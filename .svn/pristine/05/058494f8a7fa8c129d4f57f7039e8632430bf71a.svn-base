p, ul {
  margin-bottom: 0;
}

.iconfont {
  font-family: iconfont;
}

.company-video {
  width: 100%;
  min-height: 100%;
  padding: 35px 60px;
  overflow: hidden;
}

.company-video h2 {
  color: #495C60;
  font-size: 26px;
}

.company-video .eng-name {
  color: #878C90;
  font-size: 16px;
  margin-bottom: 20px;
}

.company-video .update-list {
  position: absolute;
  right: 200px;
  top: 70px;
  width: 150px;
  height: 40px;
  text-align: center;
  line-height: 40px;
  font-size: 15px;
  border-radius: 4px;
  -webkit-border-radius: 4px;
  -moz-border-radius: 4px;
  -ms-border-radius: 4px;
  -o-border-radius: 4px;
  border: 1px solid #d43f55;
  color: #d43f55;
  cursor: pointer;
}

.company-video .search-info {
  width: 100%;
  height: auto;
  overflow: hidden;
  background: #fff;
  padding: 5px 35px 25px;
  border-radius: 4px;
  -webkit-border-radius: 4px;
  -moz-border-radius: 4px;
  -ms-border-radius: 4px;
  -o-border-radius: 4px;
}

.company-video .search-info li {
  float: left;
  height: 42px;
  overflow: hidden;
  margin-right: 25px;
  line-height: 40px;
  margin-top: 20px;
}

.company-video .search-info li.last {
  float: right;
}

.company-video .search-info li .li-span {
  color: #BCBBC0;
  font-size: 14px;
  margin-right: 15px;
}

.company-video .search-info li input {
  width: 200px;
  border: 1px solid #dddddd;
  padding: 0 10px;
  border-radius: 4px;
  -webkit-border-radius: 4px;
  -moz-border-radius: 4px;
  -ms-border-radius: 4px;
  -o-border-radius: 4px;
  font-size: 14px;
  color: #495C60
}

.el-date-editor.el-range-editor.el-input__inner.el-date-editor--daterange {
  width: 410px;
  height: 42px;
}

.el-date-editor.el-range-editor.el-input__inner.el-date-editor--daterange input {
  border: none;
}

.company-video .search-info li .search-btn {
  display: inline-block;
  width: 115px;
  height: 40px;
  text-align: center;
  line-height: 40px;
  border-radius: 20px;
  -webkit-border-radius: 20px;
  -moz-border-radius: 20px;
  -ms-border-radius: 20px;
  -o-border-radius: 20px;
  background: #FB9500;
  font-size: 16px;
  color: #fff;
  cursor: pointer;
  transition: all 0.1s linear;
  -webkit-transition: all 0.1s linear;
  -moz-transition: all 0.1s linear;
  -ms-transition: all 0.1s linear;
  -o-transition: all 0.1s linear;
}

.company-video .search-info li .search-btn:hover {
  background: #fba908;
}

.company-video .search-info li .add-btn {
  display: inline-block;
  width: 90px;
  height: 40px;
  text-align: center;
  line-height: 40px;
  border: 1px solid #FB9500;
  background: #fff;
  color: #FB9500;
  cursor: pointer;
  border-radius: 20px;
  -webkit-border-radius: 20px;
  -moz-border-radius: 20px;
  -ms-border-radius: 20px;
  -o-border-radius: 20px;
  transition: all 0.1s linear;
  -webkit-transition: all 0.1s linear;
  -moz-transition: all 0.1s linear;
  -ms-transition: all 0.1s linear;
  -o-transition: all 0.1s linear;
  margin-left: 20px;
  position: relative;
}

.company-video .search-info li .disable_btn.add-btn {
  display: inline-block;
  width: 90px;
  height: 40px;
  text-align: center;
  line-height: 40px;
  border: 1px solid #ccc;
  background: #fff;
  color: #ccc;
  cursor: pointer;
  border-radius: 20px;
  -webkit-border-radius: 20px;
  -moz-border-radius: 20px;
  -ms-border-radius: 20px;
  -o-border-radius: 20px;
  margin-left: 20px;
  position: relative;
  transition: none;
  -webkit-transition: none;
  -moz-transition: none;
  -ms-transition: none;
  -o-transition: none;
}

.company-video .search-info li .add-btn:hover {
  background: #fbf4de;
}

.company-video .search-info li .add-btn.disable_btn:hover {
  background: #fff;
}

.company-video .search-info li.last-li {
  height: 50px;
}

.company-video .search-info li .sm {
  font-size: 12px;
  color: #BCBBC0;
  float: right;
  margin-left: 10px;
  width: 70px;
  line-height: 15px;
}

.company-video table {
  width: 100%;
  margin-top: 20px;
  background: #fff;
}

.company-video table th {
  background: #FAFBFC;
}

.company-video table th, .company-video table td {
  font-size: 14px;
  color: #797886;
  text-align: center;
  height: 45px;
}

.company-video table td .true {
  color: #9CD480;
}

.company-video table td .false {
  color: #F1686B;
}

.company-video table td .iconfont {
  color: #FAC583;
  font-size: 24px;
  margin-right: 20px;
  cursor: pointer;
}

.company-video table tbody tr:nth-child(even) {
  background: #FAFBFC;
}

.paging {
  margin: 20px auto;
  text-align: center;
}

.paging .el-pagination .el-pager li, .el-pagination button, .el-pagination span:not([class*=suffix]) {
  min-width: 50px;
  height: 40px;
  line-height: 40px;
  font-size: 14px;
}

.el-input--mini .el-input__inner {
  height: 40px;
  line-height: 40px;
}

.el-pagination .el-select .el-input {
  margin-left: 15px;
}

.company-video table td .p1 {
  max-width: 550px;
  margin: 0 auto;
  height: 45px;
  line-height: 45px;
  overflow: hidden;
  text-overflow: ellipsis;

}

/*.el-button {*/
/*border: none;*/
/*}*/

.el-button:hover {
  background: none;
}

.el-button--primary {
  color: #fff !important;
  background-color: #409EFF !important;
  border-color: #409EFF !important;
}

.el-range-input {
  border: none !important;
}


































































