

.iconfont {
  font-family: "iconfont" !important;
  font-size: 16px;
  font-style: normal;
  -webkit-font-smoothing: antialiased;
  -webkit-text-stroke-width: 0.2px;
  -moz-osx-font-smoothing: grayscale;
}


.up-lists-cover {
  background: rgba(0, 0, 0, 0.4);
  position: fixed;
  top: 0;
  bottom: 0;
  left: 0;
  right: 0;
  overflow-x: hidden;
  overflow-y: auto;
  z-index: 1000;
}

.up-lists-cover .cover-con {
  width: 720px;
  height: auto;
  overflow: hidden;
  background: #fff;
  margin: 50px auto;
  border-radius: 4px;
  -webkit-border-radius: 4px;
  -moz-border-radius: 4px;
  -ms-border-radius: 4px;
  -o-border-radius: 4px;
}

.up-lists-cover .cover-con h3 {
  width: 100%;
  height: 60px;
  line-height: 60px;
  text-align: center;
  font-size: 18px;
  background: #e5e5e5;
  margin: 0;
  color: #333;
}

.up-lists-cover .cover-con ul {
  width: 100%;
  height: auto;
  margin-bottom: 0 !important;
  overflow: hidden;
}

.up-lists-cover .cover-con ul li {
  border-bottom: 1px solid #eee;
  height: 70px;
  width: 100%;
  overflow: hidden;
  margin: 0 !important;
}

.up-lists-cover .cover-con ul li .icon {
  width: 100px;
  text-align: center;
  height: 40px;
  margin-top: 15px;
  float: left;
}

.up-lists-cover .cover-con ul li .upload_name {
  width: 145px;
  height: 75px;
  padding-top: 13px;
  float: left;
}

.up-lists-cover .cover-con ul li .upload_name p {
  margin: 0;
  overflow: hidden;
  text-overflow: ellipsis;
  white-space: nowrap;
  height: 20px;
  line-height: 20px;
}

.up-lists-cover .cover-con ul li .upload_name p:nth-of-type(1) {
  color: #333;
  overflow: hidden;
  text-overflow: ellipsis;
  white-space: nowrap;
}

.up-lists-cover .cover-con ul li .upload_name p:last-child {
  color: #999;
  font-size: 12px;
  margin-top: 5px;
}

.up-lists-cover .cover-con ul li .upload_process {
  float: left;
  margin-left: 10px;
  height: 15px;
  width: 280px;
  margin-top: 30px;
  background-color: #eee;
  border-radius: 15px;
  -moz-border-radius: 15px;
  -webkit-border-radius: 15px;
  -ms-border-radius: 15px;
  -o-border-radius: 15px;
}

.up-lists-cover .cover-con ul li .upload_process_in {
  height: 15px;
  background-color: #d43f55;
  border-radius: 15px;
  -moz-border-radius: 15px;
  -webkit-border-radius: 15px;
  -ms-border-radius: 15px;
  -o-border-radius: 15px;
  width: 0;
}

.up-lists-cover .cover-con ul li .upload_percent {
  float: left;
  line-height: 70px;
  padding-left: 10px;
  color: #999;
}

.up-lists-cover .cover-con ul li .upload_status {
  float: left;
  padding-top: 15px;
  margin-left: 15px;
}

.up-lists-cover .cover-con ul li .upload_status .badge {
  height: 22px;
  line-height: 20px;
  padding: 0 12px;
  font-size: 12px;
  -moz-border-radius: 3px;
  border-radius: 3px;
  font-weight: 400;
}

.up-lists-cover .cover-con ul li .upload_status .badge_success_b {
  background-color: rgba(54, 175, 71, .1);
  color: rgba(54, 175, 71, 1);
  border: 1px solid rgba(54, 175, 71, 1);
  line-height: 21px;
}
.up-lists-cover .cover-con ul li .upload_status .badge_info_b {
  background-color: rgba(81, 97, 115, .1);
  color: rgba(81, 97, 115, 1);
  border: 1px solid rgba(81, 97, 115, 1);
  line-height: 21px;
}

.up-lists-cover .cover-con ul li  .badge_primary_b {
  background-color: rgba(25, 145, 235, .1);
  color: rgba(25, 145, 235, 1);
  border: 1px solid rgba(25, 145, 235, 1);
  line-height: 21px;
}
.up-lists-cover .cover-con ul li  .badge_warning_b {
  background-color: rgba(247, 191, 71, .1);
  color: rgba(247, 191, 71, 1);
  border: 1px solid rgba(247, 191, 71, 1);
  line-height: 21px;
}

.upload_control {
  float: left;
  margin-left: 15px;
  padding-top: 23px;
}

.upload_control i {
  cursor: pointer;
}

.up-lists-cover .cover-con .remove-list {
  width: 130px;
  height: 40px;
  line-height: 40px;
  text-align: center;
  margin-top: 20px;
  margin-bottom: 20px;
  float: right;
  margin-right: 100px;
  color: #fff;
  background: #1e990a;
  font-size: 14px;
  cursor: pointer;
  border-radius: 4px;
  -webkit-border-radius: 4px;
  -moz-border-radius: 4px;
  -ms-border-radius: 4px;
  -o-border-radius: 4px;
}
