.col-sure {
  color: #7F9DE5;
}

.col-error {
  color: #F65B79;
}

.admin-video {
  min-width: 1200px;
  overflow-x: auto;
  background: #F5F8FA;
  margin-left: 250px;
  padding: 40px 16px;
  position: absolute;
  left: 0;
  top: 91px;
  bottom: 0;
  right: 0;
  overflow-y: auto;
  z-index: 10;
}

.admin-video h2,.account-cover .account-cover-con h2 {
  border-left: 5px solid #2E4769;
  text-indent: 20px;
  color: #2E4769;
  font-size: 22px;
}

.admin-video .video-classify {
  width: 100%;
  height: auto;
  padding: 0 50px 20px;
  background: #fff;
  margin-top: 40px;
  overflow: hidden;
}

.admin-video .video-classify li {
  float: left;
  height: 40px;
  margin-top: 20px;
  margin-right: 35px;
  line-height: 40px;
}

.admin-video .video-classify li span {
  color: #2E4769;
  font-size: 14px;
  margin-right: 10px;
}

.admin-video .video-classify li .inp {
  width: 220px;
  height: 40px;
  border: 1px solid #E4EBF4;
  box-sizing: border-box;
  -webkit-box-sizing: border-box;
  -moz-box-sizing: border-box;
  -ms-box-sizing: border-box;
  -o-box-sizing: border-box;
  padding: 0 10px;
  border-radius: 4px;
  -webkit-border-radius: 4px;
  -moz-border-radius: 4px;
  -ms-border-radius: 4px;
  -o-border-radius: 4px;
  color: #2E4769;
  font-size: 14px;
}

.admin-video .video-classify li.last-li {
  float: right;
}

.admin-video .video-classify li.last-li .video-up, .admin-video .video-classify li.last-li .video-search {
  display: inline-block;
  width: 110px;
  height: 40px;
  text-align: center;
  line-height: 40px;
  border-radius: 4px;
  -webkit-border-radius: 4px;
  -moz-border-radius: 4px;
  -ms-border-radius: 4px;
  -o-border-radius: 4px;
  color: #fff;
  font-size: 14px;
  cursor: pointer;
}

.admin-video .video-classify li.last-li .video-up {
  background: #71C945;
}

.admin-video .video-classify li.last-li .video-search {
  background: #4667B4;
}

.admin-video .video-list {
  border: 1px solid #F2F2F4;
  width: 100%;
  margin-top: 40px;
  background: #fff;
}

.admin-video .video-list th, .admin-video .video-list td {
  text-align: center;
  color: #2E4769;
}

.admin-video .video-list th {
  height: 60px;
  font-size: 15px;
  background: #DFE7F3;
}

.admin-video .video-list td {
  height: 50px;
  font-size: 14px;
}

.admin-video .video-list td .p1 {
  width: 350px;
  height: auto;
  margin: 0 auto;
  overflow: hidden;
  text-overflow: ellipsis;
  white-space: nowrap;
}

.admin-video .video-list td .delete-one, .admin-video .video-list td .to-edit, .admin-video .video-list td .to-look {
  display: inline-block;
  width: 70px;
  height: 30px;
  text-align: center;
  line-height: 30px;
  border-radius: 4px;
  -webkit-border-radius: 4px;
  -moz-border-radius: 4px;
  -ms-border-radius: 4px;
  -o-border-radius: 4px;
  background: #FFEEEF;
  font-size: 12px;
  cursor: pointer;
  color: #F65B79;
}

.admin-video .video-list td .to-edit {
  background: #A6BCD9;
  color: #4667B4;
  margin-right: 15px;
}

.admin-video .video-list td .to-look {
  background: #FFF4D5;
  color: #EFB920;
}

.admin-video .video-list tbody tr:nth-child(even) {
  background: #F8FAFE;
}

.paging {
  margin: 20px auto;
  text-align: center;
}

.paging .el-pagination .el-pager li, .el-pagination button, .el-pagination span:not([class*=suffix]) {
  min-width: 50px;
  height: 40px;
  line-height: 40px;
  font-size: 14px;
}
.account-cover{
  position: fixed;
  left: 0;
  top: 0;
  right: 0;
  bottom: 0;
  overflow-y: auto;
  background: rgba(0,0,0,0.4);
  z-index: 100;
  width: 100%;
}
.account-cover .account-cover-con{
  width: 1000px;
  height: auto;
  background: #fff;
  border-radius: 6px;
  -webkit-border-radius: 6px;
  -moz-border-radius: 6px;
  -ms-border-radius: 6px;
  -o-border-radius: 6px;
  box-sizing: border-box;
  -webkit-box-sizing: border-box;
  -moz-box-sizing: border-box;
  -ms-box-sizing: border-box;
  -o-box-sizing: border-box;
  padding: 40px 50px;
  margin: 150px auto;
  position: relative;
}
.account-cover .account-cover-con .close{
  display: block;
  position: absolute;
  right: 40px;
  top: 20px;
  cursor: pointer;
}
.account-cover .account-cover-con .cover-detail{
  width: 520px;
  height: auto;
  overflow: hidden;
  margin: 0 auto;
  padding: 2px 0;
  box-sizing: border-box;
  -webkit-box-sizing: border-box;
  -moz-box-sizing: border-box;
  -ms-box-sizing: border-box;
  -o-box-sizing: border-box;
}
.account-cover .account-cover-con .cover-detail li{
  width: 100%;
  height: 40px;
  line-height: 40px;
  margin-top: 20px;
}
.account-cover .account-cover-con .cover-detail li label{
  font-size: 12px;
  color: #F65B79;
  margin-left: 10px;
}
.account-cover .account-cover-con .cover-detail li span{
  float: left;
  width: 65px;
  height: 40px;
  text-align: right;
  color: #2E4769;
  font-size: 14px;
}
.account-cover .account-cover-con .cover-detail li span label{
  margin-left: 0;
}
.account-cover .account-cover-con .cover-detail li input{
  width: 300px;
  height: 40px;
  border: 1px solid #E4EBF4;
  border-radius: 4px;
  -webkit-border-radius: 4px;
  -moz-border-radius: 4px;
  -ms-border-radius: 4px;
  -o-border-radius: 4px;
  box-sizing: border-box;
  -webkit-box-sizing: border-box;
  -moz-box-sizing: border-box;
  -ms-box-sizing: border-box;
  -o-box-sizing: border-box;
  padding: 0 10px;
  float: left;
  margin-left: 20px;
  color: #2E4769;
  font-size: 14px;
}
.account-cover .account-cover-con .account-save{
  width: 260px;
  height: 50px;
  text-align: center;
  line-height: 50px;
  margin: 100px auto 30px;
  background: #71C945;
  border-radius: 4px;
  -webkit-border-radius: 4px;
  -moz-border-radius: 4px;
  -ms-border-radius: 4px;
  -o-border-radius: 4px;
  color: #fff;
  font-size: 16px;
  cursor: pointer;
}
.el-pagination .el-select .el-input .el-input__inner{
  height: 40px;
}
