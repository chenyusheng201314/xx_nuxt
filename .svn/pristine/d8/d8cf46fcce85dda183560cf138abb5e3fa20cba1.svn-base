p {
  margin-bottom: 0;
}

.company {
  width: 100%;
  height: 100%;
  background: #F9F9FC;
  padding: 35px 16px;
  overflow-y: auto;
}

.company h2 {
  color: #495C60;
  font-size: 26px;
}

.company .eng-name {
  color: #878C90;
  font-size: 16px;
  margin-bottom: 20px;
}

.el-row {
  margin-bottom: 20px;
}

el-row:last-child {
  margin-bottom: 0;
}

.el-col {
  border-radius: 4px;
}

.bg-purple {
  background: #fff;
}

.grid-content {
  border-radius: 4px;
  -webkit-border-radius: 4px;
  -moz-border-radius: 4px;
  -ms-border-radius: 4px;
  -o-border-radius: 4px;
  min-height: 36px;
}

.row-bg {
  padding: 10px 0;
  background-color: #f9fafc;
}

.company .info-top {
  width: 100%;
  height: 70px;
  line-height: 70px;
  background: #fff;
  border-bottom: 1px solid #DDDDDD;
}

.company .info-top .span-left {
  float: left;
  margin-left: 30px;
  font-size: 16px;
  color: #495C60;
}

.company .info-top .span-right {
  float: right;
  margin-right: 50px;
  color: #FB9500;
  font-size: 16px;
  cursor: pointer;
}

.company .info-bottom {
  width: 100%;
  min-height: 340px;
  background: #fff;
  padding: 40px 30px;
  margin-bottom: 0;
}

.company .info-bottom li {
  width: 100%;
  height: auto;
  overflow: hidden;
  margin-top: 20px;
}

.company .info-bottom li:first-child {
  margin-top: 0;
}

.company .info-bottom li .li-name {
  width: 80px;
  overflow: hidden;
  float: left;
  color: #BCBBC0;
  font-size: 14px;
}

.company .info-bottom li .li-name.li-name2 {
  width: 110px;
}

.company .info-bottom li .li-name.edit {
  height: 40px;
  line-height: 40px;
}

.company .info-bottom li .li-con {
  width: calc(100% - 80px);
  overflow: hidden;
  float: left;
  color: #495C60;
  font-size: 14px;
  padding-left: 10px;
}

.company .info-bottom li .li-con.li-con2 {
  width: calc(100% - 110px);
}

.company .info-bottom li .li-con img {
  width: 80px;
  height: 80px;
  border-radius: 40px;
  -webkit-border-radius: 40px;
  -moz-border-radius: 40px;
  -ms-border-radius: 40px;
  -o-border-radius: 40px;
}

.company .info-bottom li .li-con input {
  width: 100%;
  height: 40px;
  border: 1px solid #DDDDDD;
  border-radius: 4px;
  -webkit-border-radius: 4px;
  -moz-border-radius: 4px;
  -ms-border-radius: 4px;
  -o-border-radius: 4px;
  padding: 0 10px;
}

.company .news-list {
  width: 100%;
  height: auto;
  overflow: hidden;
  padding: 10px 40px 30px;
  background: #fff;
  margin-bottom: 0;
}

.company .news-list li {
  width: 48%;
  height: 75px;
  overflow: hidden;
  float: left;
  margin-top: 20px;
  cursor: pointer;
}

.company .news-list li:nth-child(2n) {
  margin-left: 2%;
}

.company .news-list li .new-left {
  float: left;
  margin-right: 20px;
  height: auto;
  width: 114px;
}

.company .news-list li .new-left img {
  width: 114px;
  height: 72px;
}

.company .news-list li .new-right {
  float: left;
  height: auto;
  width: calc(100% - 134px);
}

.company .news-list li .new-right .news-tit, .company .news-list li .new-right .news-con {
  color: #495C60;
  font-size: 14px;
  width: 100%;
  overflow: hidden;
  text-overflow: ellipsis;
  white-space: nowrap;
  margin-bottom: 3px;
}

.company .news-list li .new-right .news-con {
  color: #BCBBC0;
}

.el-dialog {
  border-radius: 4px;
}

.el-dialog .el-dialog__title {
  border-left: none !important;
  padding-left: 0 !important;
}

.el-dialog__header {
  border-bottom: 1px solid #dddddd;
  padding: 20px
}

.new-cover-con {
  background: #fff;
  padding: 40px 40px 10px;
}

.new-cover-con .con-tit {
  text-align: center;
  width: 100%;
  font-size: 20px;
  color: #495C60;
  margin-bottom: 5px;
}

.new-cover-con .con-date {
  text-align: center;
  width: 100%;
  font-size: 14px;
  color: #dddddd;
  margin-bottom: 20px;
}
