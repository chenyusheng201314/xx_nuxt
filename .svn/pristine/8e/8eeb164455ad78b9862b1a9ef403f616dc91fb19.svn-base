.load_block {
    display: inline-block;
    width: 10px;
    height: 10px;
    background-color: #00dafe;
    margin-left: 10px;
    -webkit-transform-origin: right bottom;
    -moz-transform-origin: right bottom;
    -ms-transform-origin: right bottom;
    transform-origin: right bottom;
    -webkit-animation: load_animate 1.3s ease infinite;
    -moz-animation: load_animate 1.3s ease infinite;
    animation: load_animate 1.3s ease infinite;
}

.load_block:nth-of-type(1) {
    -webkit-animation-delay: 0.13s;
    -moz-animation-delay: 0.13s;
    animation-delay: 0.13s;
    margin-left: 0;
}

.load_block:nth-of-type(2) {
    -webkit-animation-delay: 0.26s;
    -moz-animation-delay: 0.26s;
    animation-delay: 0.26s;
}

.load_block:nth-of-type(3) {
    -webkit-animation-delay: 0.39s;
    -moz-animation-delay: 0.39s;
    animation-delay: 0.39s;
}

.load_block:nth-of-type(4) {
    -webkit-animation-delay: 0.52s;
    -moz-animation-delay: 0.52s;
    animation-delay: 0.52s;
}

.load_block:nth-of-type(5) {
    -webkit-animation-delay: 0.65s;
    -moz-animation-delay: 0.65s;
    animation-delay: 0.65s;
}

.load_remove {
    border: 0;
    pointer-events: none;
    padding-left: 5px !important;
}

.load_remove:hover {
    pointer-events: none;
    -webkit-transition: all .3s ease-in-out;
    -moz-transition: all .3s ease-in-out;
    transition: all .3s ease-in-out;
}

@-webkit-keyframes load_animate {

    0% {
        opacity: 1;
    }
    100% {
        -webkit-transform: rotate(90deg);
        transform: rotate(90deg);
        opacity: 0;
    }
}

