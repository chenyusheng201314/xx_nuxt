.edit-course {
  width: 100%;
  height: auto;
  overflow: hidden;
  padding: 30px 70px 50px;
}

.edit-course .edit-info .li {
  margin-top: 20px;
  position: relative;
}

.edit-course .edit-info .li .reach-edit {
  width: 100%;
  height: auto;
  overflow: hidden;
}

.edit-course .edit-info .li span.type-name {
  display: inline-block;
  height: 40px;
  line-height: 40px;
  text-align: right;
  width: 80px;
  margin-right: 15px;
  font-size: 14px;
  color: #BCBBC0;
}

.edit-course .edit-info .li span.type-name label {
  color: #F65B79;
  font-size: 14px;
}

.edit-course .edit-info .li input {
  width: 300px;
  height: 40px;
  border: 1px solid #dddddd;
  box-sizing: border-box;
  -webkit-box-sizing: border-box;
  -moz-box-sizing: border-box;
  -ms-box-sizing: border-box;
  -o-box-sizing: border-box;
  padding: 0 10px;
  border-radius: 4px;
  -webkit-border-radius: 4px;
  -moz-border-radius: 4px;
  -ms-border-radius: 4px;
  -o-border-radius: 4px;
  color: #2E4769;
  font-size: 15px;
}

.edit-course .edit-info .li .class-time {
  color: #2E4769;
  font-size: 14px;
  margin-left: 5px;
}

.edit-course .course-section {
  width: 100%;
  height: auto;
  overflow: hidden;
  margin-top: 20px;
  border: 1px solid #D7D7D7;
}

.edit-course .course-section .section-title {
  width: 100%;
  height: 70px;
  background: #FAFBFC;
  padding: 0 20px;
  line-height: 70px;
  position: relative;
}

.edit-course .course-section .section-title .delete-one {
  font-size: 20px;
  color: #F65B79;
  position: absolute;
  right: 0;
  top: 0;
  width: 20px;
  height: 20px;
  font-family: iconfont;
  line-height: 20px;
  cursor: pointer;
}

.edit-course .course-section .section-title .round {
  display: inline-block;
  width: 12px;
  height: 12px;
  border-radius: 12px;
  -webkit-border-radius: 12px;
  -moz-border-radius: 12px;
  -ms-border-radius: 12px;
  -o-border-radius: 12px;
  background: #FB9500;
  margin-right: 10px;
}

.edit-course .course-section .section-title .txt {
  margin-right: 10px;
  font-size: 16px;
  color: #495C60;
}

.edit-course .course-section .section-title input {
  width: 230px;
  height: 40px;
  border: 1px solid #dddddd;
  border-radius: 4px;
  -webkit-border-radius: 4px;
  -moz-border-radius: 4px;
  -ms-border-radius: 4px;
  -o-border-radius: 4px;
  padding: 0 10px;
}

.edit-course .course-section .section-title .add-sec {
  float: right;
  width: 130px;
  height: 40px;
  text-align: center;
  line-height: 40px;
  margin-top: 15px;
  border-radius: 20px;
  -webkit-border-radius: 20px;
  -moz-border-radius: 20px;
  -ms-border-radius: 20px;
  -o-border-radius: 20px;
  font-size: 16px;
  color: #FB9500;
  border: 1px solid #FB9500;
  cursor: pointer;
  transition: all 0.1s linear;
  -webkit-transition: all 0.1s linear;
  -moz-transition: all 0.1s linear;
  -ms-transition: all 0.1s linear;
  -o-transition: all 0.1s linear;
  margin-right: 40px;
}

.edit-course .course-section .section-title .add-sec:hover {
  background: #fbf4de;
}

.edit-course .course-section .section-list {
  width: 100%;
  height: 50px;
  border-bottom: 1px solid #ddd;
  line-height: 50px;
  background: #fff;
  padding: 0 30px;
}

.edit-course .course-section .section-list:last-child {
  border-bottom: none;
}

.edit-course .course-section .section-list .li-txt, .edit-course .course-section .section-list .li-txt2 {
  display: inline-block;
  width: 80px;
  height: auto;
  color: #495C60;
  font-size: 14px;
}

.edit-course .course-section .section-list .li-txt2 {
  width: 120px;
}

.edit-course .course-section .section-list input {
  width: 250px;
  height: 35px;
  margin-top: 4px;
  border: 1px solid #dddddd;
  border-radius: 4px;
  -webkit-border-radius: 4px;
  -moz-border-radius: 4px;
  -ms-border-radius: 4px;
  -o-border-radius: 4px;
  padding: 0 10px;
  margin-right: 30px;
}

.edit-course .course-section .section-list .iconfont {
  color: #FAC583;
  font-size: 24px;
  margin-right: 20px;
  cursor: pointer;
  float: right;
  width: 100px;
  text-align: center;
}

.edit-course .add-fir {
  width: 130px;
  height: 40px;
  text-align: center;
  line-height: 40px;
  border-radius: 20px;
  -webkit-border-radius: 20px;
  -moz-border-radius: 20px;
  -ms-border-radius: 20px;
  -o-border-radius: 20px;
  border: 1px solid #FB9500;
  color: #FB9500;
  margin-top: 20px;
  font-size: 16px;
  cursor: pointer;
}

.edit-course .add-save {
  width: 240px;
  height: 50px;
  background: #71C945;
  color: #fff;
  font-size: 16px;
  text-align: center;
  line-height: 50px;
  border-radius: 50px;
  -webkit-border-radius: 50px;
  -moz-border-radius: 50px;
  -ms-border-radius: 50px;
  -o-border-radius: 50px;
  margin: 50px auto;
  cursor: pointer;
  transition: all 0.1s linear;
}

.edit-course .add-save:hover {
  background: #87c96e;
}

.edit-course .class-info {
  width: 100%;
  height: 180px;
  padding: 20px;
  background: #FBFBFD;
  overflow: hidden;
  box-sizing: content-box;
  -webkit-box-sizing: content-box;
  -moz-box-sizing: content-box;
  -ms-box-sizing: content-box;
  -o-box-sizing: content-box;
}

.edit-course .class-info .info-left {
  width: 285px;
  height: 180px;
  float: left;
  overflow: hidden;
  margin-right: 20px;
}

.edit-course .class-info .info-left img {
  width: 285px;
  height: 180px;
}

.edit-course .class-info .info-right {
  width: -moz-calc(100% - 305px);
  width: -webkit-calc(100% - 305px);
  float: left;
  height: 180px;
  overflow: hidden;
}

.edit-course .class-info .info-right .tit {
  width: 100%;
  color: #495C60;
  font-size: 16px;
  text-overflow: ellipsis;
  overflow: hidden;
  white-space: nowrap;
}

.edit-course .class-info .info-right .p {
  width: 100%;
  height: auto;
  margin-top: 5px;
}

.edit-course .class-info .info-right .p .iconfont {
  color: #FB9500;
  font-size: 20px;
  margin-right: 10px;
}

.edit-course .class-info .info-right .p .span {
  font-size: 14px;
  color: #878C90;
  margin-right: 20px;
}

.edit-course .class-info .info-right .tip {
  width: 100px;
  font-size: 14px;
  padding: 5px;
  color: #6FB6FF;
  background: #EDF4FD;
  box-sizing: content-box;
  -webkit-box-sizing: content-box;
  -moz-box-sizing: content-box;
  -ms-box-sizing: content-box;
  -o-box-sizing: content-box;
  border-radius: 2px;
  -webkit-border-radius: 2px;
  -moz-border-radius: 2px;
  -ms-border-radius: 2px;
  -o-border-radius: 2px;
  margin-top: 20px;
}

.edit-course .class-info .info-right .tip .iconfont {
  font-size: 24px;
  float: left;
  margin-top: -7px;
}

.edit-course .course-nav {
  margin-bottom: 20px;
}

.edit-course .see-list {
  width: 100%;
  height: auto;
  overflow: hidden;
  border: 1px solid #ddd;
  border-radius: 4px;
  -webkit-border-radius: 4px;
  -moz-border-radius: 4px;
  -ms-border-radius: 4px;
  -o-border-radius: 4px;
  margin-top: 10px;
}

.edit-course .see-list li {
  width: 100%;
  height: 50px;
  line-height: 50px;
  border-bottom: 1px solid #ddd;
  background: #fff;
  padding: 0 30px;
}

.edit-course .see-list li:last-child {
  border-bottom: none;
}

.edit-course .see-list .tit {
  height: 70px;
  line-height: 70px;
  background: #FAFBFC;
  padding: 0 20px;
}

.edit-course .see-list .tit .round {
  width: 12px;
  height: 12px;
  border-radius: 12px;
  display: inline-block;
  background: #FB9500;
  margin-right: 20px;
}

.edit-course .see-list .tit .txt {
  color: #495C60;
  font-size: 16px;
  margin-right: 20px;
}

.edit-course .see-list .li span {
  font-size: 14px;
  color: #495C60;
  display: inline-block;
  overflow: hidden;
  text-overflow: ellipsis;
  white-space: nowrap;
}

.edit-course .see-list .li .span1 {
  width: 15%;
}

.edit-course .see-list .li .span2 {
  width: 65%;
}

.edit-course .see-list .li .span3 {
  width: 20%;
  text-align: center;
}

.el-date-editor.el-range-editor.el-input__inner.el-date-editor--daterange {
  height: 38px;
  width: 410px;
}

.el-date-editor.el-range-editor.el-input__inner.el-date-editor--daterange input {
  border: none;
  height: 36px;

}

.edit-course table tr .iconfont {
  color: #FB9500;
  font-size: 24px;
  cursor: pointer;
  font-weight: normal;
  margin-right: 0;
}

.edit-course table tr td img {
  width: 36px;
  height: 36px;
  border-radius: 36px;
  -webkit-border-radius: 36px;
  -moz-border-radius: 36px;
  -ms-border-radius: 36px;
  -o-border-radius: 36px;
  margin-right: 10px;
}

.edit-course table tr td .true {
  color: #fba908;
}

.edit-course table tr td .false {
  color: #F65B79;
}

.edit-course table tr td .name {
  width: 300px;
  height: 45px;
  line-height: 45px;
  overflow: hidden;
  text-overflow: ellipsis;
  white-space: nowrap;
  margin: 0 auto;
}

.edit-course .out-save {
  width: 250px;
  height: 50px;
  line-height: 50px;
  text-align: center;
  border-radius: 30px;
  -webkit-border-radius: 30px;
  -moz-border-radius: 30px;
  -ms-border-radius: 30px;
  -o-border-radius: 30px;
  background: #71C945;
  margin: 50px auto;
  color: #fff;
  font-size: 16px;
  cursor: pointer;
  transition: all 0.2s linear;
}

.edit-course .out-save:hover {
  background: #85c95f;
}
.dis_choose{
  display: inline-block;
