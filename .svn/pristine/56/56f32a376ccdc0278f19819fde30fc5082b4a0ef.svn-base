.el-button--primary {
  color: #fff !important;
  background-color: #409EFF !important;
  border-color: #409EFF !important;
  border-radius: 3px !important;
  height: auto !important;
  width: auto !important;
}

p, ul {
  margin-bottom: 0;
}

.iconfont {
  font-family: iconfont;
}

.txt-center {
  text-align: center;
}

.company-course .search-info li .el-range-input {
  border: none !important;
}

.company-course {
  width: 100%;
  min-height: 100%;
  padding: 35px 60px;
  overflow: hidden;
}

.company-course h2 {
  color: #495C60;
  font-size: 26px;
}

.company-course .eng-name {
  color: #878C90;
  font-size: 16px;
  margin-bottom: 20px;
}

.company-course .search-info {
  width: 100%;
  height: auto;
  overflow: hidden;
  background: #fff;
  padding: 5px 35px 25px;
  border-radius: 4px;
  -webkit-border-radius: 4px;
  -moz-border-radius: 4px;
  -ms-border-radius: 4px;
  -o-border-radius: 4px;
}

.company-course .search-info li {
  float: left;
  height: 42px;
  overflow: hidden;
  margin-right: 40px;
  line-height: 40px;
  margin-top: 20px;
}

.company-course .search-info li.last-li {
  float: right;
}

.company-course .search-info li .li-span {
  color: #BCBBC0;
  font-size: 14px;
  margin-right: 15px;
}

.company-course .search-info li input {
  width: 200px;
  height: 38px;
  border: 1px solid #dddddd;
  padding: 0 10px;
  border-radius: 4px;
  -webkit-border-radius: 4px;
  -moz-border-radius: 4px;
  -ms-border-radius: 4px;
  -o-border-radius: 4px;
  font-size: 14px;
  color: #495C60
}

.company-course .search-info li .search-btn {
  display: inline-block;
  width: 115px;
  height: 40px;
  text-align: center;
  line-height: 40px;
  border-radius: 20px;
  -webkit-border-radius: 20px;
  -moz-border-radius: 20px;
  -ms-border-radius: 20px;
  -o-border-radius: 20px;
  background: #FB9500;
  font-size: 16px;
  color: #fff;
  cursor: pointer;
  transition: all 0.1s linear;
  -webkit-transition: all 0.1s linear;
  -moz-transition: all 0.1s linear;
  -ms-transition: all 0.1s linear;
  -o-transition: all 0.1s linear;
}

.company-course .search-info li .search-btn:hover {
  background: #fba908;
}

.company-course .search-info li .add-btn {
  display: inline-block;
  width: 160px;
  height: 40px;
  text-align: center;
  line-height: 40px;
  border: 1px solid #FB9500;
  background: #fff;
  color: #FB9500;
  cursor: pointer;
  border-radius: 20px;
  -webkit-border-radius: 20px;
  -moz-border-radius: 20px;
  -ms-border-radius: 20px;
  -o-border-radius: 20px;
  transition: all 0.1s linear;
  -webkit-transition: all 0.1s linear;
  -moz-transition: all 0.1s linear;
  -ms-transition: all 0.1s linear;
  -o-transition: all 0.1s linear;
  margin-left: 20px;
}

.company-course .search-info li .add-btn:hover {
  background: #fbf4de;
}

.course-nav {
  width: 100%;
  height: auto;
  overflow: hidden;
  margin-top: 30px;
}

.course-nav .nav-con {
  width: 140px;
  height: 40px;
  display: inline-block;
  text-align: center;
  line-height: 40px;
  font-size: 16px;
  color: #878C90;
  margin-right: 30px;
  cursor: pointer;
}

.course-nav .nav-con.nav_active {
  background: #FB9500;
  border-radius: 20px;
  -webkit-border-radius: 20px;
  -moz-border-radius: 20px;
  -ms-border-radius: 20px;
  -o-border-radius: 20px;
  color: #fff;
}

.company-course table {
  width: 100%;
  margin-top: 20px;
  background: #fff;
}

.company-course table th {
  background: #FAFBFC;
}

.company-course table th, .company-course table td {
  font-size: 14px;
  color: #797886;
  text-align: center;
  height: 45px;
}

.company-course table td .true {
  color: #9CD480;
}

.company-course table td .false {
  color: #F1686B;
}

.company-course table td .iconfont {
  color: #FAC583;
  font-size: 24px;
  margin-right: 20px;
  cursor: pointer;
}

.company-course table td .p1 {
  max-width: 650px;
  height: 40px;
  line-height: 40px;
  overflow: hidden;
  text-overflow: ellipsis;
  white-space: nowrap;
  margin: 0 auto;
}

.company-course table tbody tr:nth-child(even) {
  background: #FAFBFC;
}

.paging {
  margin: 20px auto;
  text-align: center;
}

.paging .el-pagination .el-pager li, .el-pagination button, .el-pagination span:not([class*=suffix]) {
  min-width: 50px;
  height: 40px;
  line-height: 40px;
  font-size: 14px;
}

.el-input--mini .el-input__inner {
  height: 40px;
  line-height: 40px;
}

.el-pagination .el-select .el-input {
  margin-left: 15px;
}

.el-dialog {
  border-radius: 4px;
}

.el-dialog .el-dialog__title {
  border-left: none !important;
  padding-left: 0 !important;
  font-size: 16px;
  color: #495C60;
}

.el-dialog__header {
  border-bottom: 1px solid #dddddd;
  padding: 20px
}

